Temperature = 300.0

DataSummaryFile = ../data/data_summary.log

# Record of biases used
BiasingParametersFile = biasing_parameters.log

# x = Ntilde_v
OrderParameter = {
	Name       = x
	# Location of files list, and which column contans the file locations
	FilesList  = [ time_series_files_list.input  2 ]
	# Expected format: t(ps), N_v, Ntilde_v
	DataColumn = 3

	# Binning
	Bins = {
		NumBins  = 35
		BinRange = [ 0.0  35.0 ]
		BinStyle = left
	}
}

# y = N_v
OrderParameter = {
	Name       = y
	# Location of files list, and which column contans the file locations
	FilesList  = [ time_series_files_list.input  2 ]
	# Expected format: t(ps), N_v, Ntilde_v
	DataColumn = 2

	# Binning
	Bins = {
		NumBins  = 35
		BinRange = [ 0.0  35.0 ]
		BinStyle = left
	}
}
